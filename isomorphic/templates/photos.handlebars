<div class="pure-g-r">
	<div class="pure-u-2-3 text-box">
	    <div class="l-box">
	        <h1 class="text-box-head">Photos of recife.</h1>
	        <p class="text-box-subhead">Explore photos of the recife area.</p>
	        <p class="text-box-byline">Powered by Yahoo open source technology, and photos from the Flickr.</p>
	        <p class="text-box-byline">Built as part of the feecbr workshop.</p>
	    </div>
	</div>
	{{#each items}}
		<div class="pure-u-1-3 photo-box">
		    <a href="{{ url }}">
		        <img src="{{ url }}"
		             alt="{{ title }}">
		    </a>
		    <aside class="photo-box-caption">
		        {{ title }}
		        <span>
		            by <a href="{{ url }}">{{ user }}</a> / <a href="http://creativecommons.org/licenses/by-nc-sa/2.0/">cc</a>
		        </span>
		    </aside>
		</div>
	{{/each}}
</div>
